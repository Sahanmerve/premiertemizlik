    <script>
        function DetailOpen(ID) {
            $('.slide_portfolio').slideUp();
            $('#portfolio' + ID).slideDown();
            $('html, body').animate({
                scrollTop:
                    $('#portfolio_detail').offset().top
            }, 800);
        }
        var owl = $('.owl-carousel');
        owl.owlCarousel({
            items: 1,
            loop: true,
            margin: 10,
            autoplay: true,
            autoplayTimeout: 2000,
            autoplayHoverPause: true
        });
