    <link rel="stylesheet" href="/Content/css/hoverex-all.css">
    <!-- Hover Info on Member /-->
    <script src="/Content/js/modernizr-2.6.2.min.js"></script>  <!-- Modernizr /-->


</head>


<body>
    <div class="container">

        <div class="mobile-menu">
            <section id="collapse">
                <div class="row">
                    <div class="mobile-menu-inner">

                        <ul class="nav-mobile">
                            <li><a class="selected" href="#works">YAPTIĞIMIZ İŞLER</a></li>
                            <li><a href="#whatwedo">BİZ NE YAPIYORUZ?</a></li>
                            <li><a href="#team">HAKKIMIZDA</a></li>
                            <li><a href="#contact">TALEP FORMU</a></li>
                            @if (User.IsInRole("Admin"))
                            {
                                <li><a href="#admin" class="selected">ADMİN</a></li>
                            }
                            else
                            {
                                <li><a href="#login" class="selected">GİRİŞ</a></li>
                            }
                        </ul>
                    </div>
                </div>
            </section>
            <a href="#" id="collapse-menu"><h3>MENU</h3></a>
        </div>


        <!-- NAVIGATION ############################################### -->


        <div class="cbp-af-header">

            <nav class="row">
                <div id="logo"><a href="#"><img alt="YOURLOGO" src="~/Content/WhatsApp Image 2019-08-07 at 18.08.15.jpeg"></a></div>

                <ul id="nav">
                    <li><a href="#contact">TALEP FORMU</a></li>
                    <li><a href="#team">HAKKIMIZDA</a></li>
                    <li><a href="#whatwedo">BİZ NE YAPIYORUZ?</a></li>
                    <li><a class="selected" href="#works">YAPTIĞIMIZ İŞLER</a></li>
                    @if (User.IsInRole("Admin"))
                    {
                        <li><a href="#admin" class="selected">ADMİN</a></li>
                    }
                    else
                    {
                        <li><a href="#login" class="selected">GİRİŞ</a></li>
                    }
                </ul>
            </nav>


        </div>
        <!-- END NAVIGATION ############################################### -->
        <!-- HEADER ############################################### -->
        <section id="header">

            <div class="row">

                <div class="twelve columns centered">

                    <h2 class="fade-it2">@ViewBag.GeneralSettings.HeaderTitle</h2>

                    <h2 class="fade-it2">@ViewBag.GeneralSettings.HeaderSubtitle</h2>

                </div>

            </div>

        </section>

        <!-- END HEADER ############################################### -->
        <!-- WORKS SECTION ############################################### -->
        <!-- Title /-->
        <div class="row">

            <div class="twelve columns">

                <h3>YAPTIĞIMIZ İŞLER</h3>

            </div>

        </div>
        <!-- Grid of Works /-->
        @Html.Action("_Works")
        <!-- END WORKS SECTION ############################################### -->
        <!-- INTERSECTION ############################################### -->
        @Html.Action("_General")
        <!-- END INTERSECTION ############################################### -->
        <!-- SERVICES SECTION ############################################### -->


        <div class="row">

            <div class="twelve columns">

                <h3>BİZ NE YAPIYORUZ?</h3>

            </div>

        </div>

        @Html.Action("_Services")

        <!-- END SERVICES SECTION ############################################### -->
        <!-- TEAM SECTION ############################################### -->
        <!-- Title /-->


        <div class="row">

            <div class="twelve columns">

                <h3>ÇALIŞMA ARKADAŞLARIMIZ</h3>

            </div>

        </div>


        @Html.Action("_Team")

        <!-- END TEAM SECTION ############################################### -->
        <!-- INTERSECTION ( TEMPLATE DOWNLOAD ) ############################################### -->

        <section id="interblock">

            <div class="row">

                <div class="nine columns">

                    <h2 class="fade-it2">@ViewBag.GeneralSettings.BennarTitle</h2>

                    <p class="fade-it2">@ViewBag.GeneralSettings.BennarSubtitle</p>

                </div>

                <div class="three columns buttongo">

                    <a href="#whatwedo"><i class='icon' data-icon="&#xe105;"></i></a>

                </div>


            </div>

        </section>



        <!-- END INTERSECTION ############################################### -->
        <!-- CONTACT SECTION ############################################### -->
        <!-- Title /-->


        <div class="row">

            <div class="twelve columns">

                <h3>TALEP FORMU</h3>

            </div>

        </div>

        <section id="contact">

            <div class="row contact">

                <div class="twelve columns centered">

                    <!-- CONTACT FORM -->

                    <form name="htmlform" method="post">

                        <div class="five columns">

                            <input type="text" name="name" placeholder="AD SOYAD" required>
                            <input type="text" name="phone" placeholder="TELEFON NUMARASI" required>
                            <input type="email" name="email" placeholder="E-MAİL" required>

                        </div>

                        <div class="seven columns">

                            <textarea name="comments" cols="1" rows="5" placeholder="AÇIKLAMANIZ" required></textarea>

                        </div>

                        <button name="send" type="submit" class="submit">GÖNDER</button>


                    </form>

                </div>

            </div>

        </section>
        @Html.Action("_Login")
        <br /><br />

        <!-- END CONTACT SECTION ############################################### -->
        <!-- FOOTER INTERSECTION SECTION ############################################### -->


        <section id="interblock_footer">

            <div class="row">

                <div class="eight columns">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3008.3172495658455!2d28.985625215415755!3d41.06205747929539!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zNDHCsDAzJzQzLjQiTiAyOMKwNTknMTYuMSJF!5e0!3m2!1str!2str!4v1566560390901!5m2!1str!2str" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>

                </div>

                <!-- Social Icon /-->

                <div class="four columns text-right ">

                    <ul>

                        <li><a href="https://www.facebook.com/premiertemizlikveorganizasyon/" target="_blank"><i class="icon" data-icon="&#xe045;"></i></a></li>

                        <li><a href="https://www.instagram.com/_premier_0/?hl=tr" target="_blank"><i class='icon' data-icon="&#xe03e;"></i></a></li>

                        <li><a href="https://api.whatsapp.com/send?phone=902129095869" target="_blank"><i class='icon' data-icon="&#xe022;"></i></a></li>
                        <li>
                            <a href="mailto:premier@destekpremier.com" class="email" target="_blank"><i class="icon" data-icon="&#xe048;"></i></a>
                        </li>
                    </ul>
                    <a href="https://www.google.com.tr/maps/" target="_blank"><p>Merkez Mahallesi,Abide-i Hürriyet Caddesi,Sibel Apartmanı,No:161,Kat:2/3 Şişli/İstanbul</p></a>
                </div>
                
            </div>


        </section>


        <!-- END FOOTER INTERSECTION ############################################### -->
        <!-- FOOTER CONTACT INFO SECTION ############################################### -->


        <section id="contact_info">

            <div class="row">

                <div class="twelve columns">

                    <div class="four columns text-center">
                        <p>
                            <a href="tel:+902129095869" class="Num">
                                <i class="icon" data-icon="&#xe022;">
                                </i>
                                0212 909 58 69
                            </a>
                        </p>
                    </div>
                   
                        
                    

                </div>

            </div>

        </section>




        <!-- END FOOTER CONTACT INFO SECTION ############################################### -->


    </div> <!-- end of #container -->
